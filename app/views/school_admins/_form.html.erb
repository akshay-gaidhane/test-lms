<!-- <form> -->
<%= form_with(scope: :school, url: school_admin_path(@school), method: :patch) do |form| %>
  <% if school.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(school.errors.count, "error") %> prohibited this school from being saved:</h2>

      <ul>
        <% school.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="mb-3">
    <%= form.label :name, class: "form-label" %>
    <%= form.text_field :name, disabled: true, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= form.label :school_admin_id, class: "form-label" %>
    <%= form.select :school_admin_id, User.all.map{|l| [l.email, l.id]}, { prompt: "Please Select" }, class: 'form-select form-select-lg mb-3' %>
  </div>

  <div class="actions">
    <%= form.submit class: "btn btn-primary" %>
  </div>
<% end %>
<!-- </form> -->